{
    "name": "rooooodik/yii2-amqp",
    "description": "Yii 2 extension wrapper to communicate with RabbitMQ server via AMQP. Based on videlalvaro/php-amqplib.",
    "keywords": ["yii", "extension", "amqp", "RabbitMQ"],
    "homepage": "https://github.com/webtoucher/yii2-amqp",
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rodion Konskii",
            "email": "rodion.kosnkii@gmail.ru"
        },
        {
          "name": "Alexey Kuznetsov",
          "email": "mirakuru@webtoucher.ru"
        }
    ],
    "replace": {
        "webtoucher/yii2-amqp":"*"
    },
    "require": {
        "php": ">=5.3.0",
        "php-amqplib/php-amqplib": "~2.0",
        "webtoucher/yii2-commands": "*",
        "yiisoft/yii2": ">=2.0.6"
    },
    "autoload": {
        "psr-4": {
            "rooooodik\\amqp\\": ""
        }
    }
}
